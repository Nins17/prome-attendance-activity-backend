generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Attendance {
  id       Int    @id @default(autoincrement())
  ref_id   String @unique @db.VarChar(10)
  fullname String
  schedule String
}

model Admin {
  id        Int   @id @default(autoincrement())
  username  String
  password  String
}