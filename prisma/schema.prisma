generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Attendance {
  id       Int    @id @default(autoincrement())
  fullname String
  schedule String
}

model Admin {
  id        Int   @id @default(autoincrement())
  username  String
  password  String
}